
<div class="header">{{t 'room.listOfRoomsHeader'}} {{selectedLibrary}} {{convert-to-lower-caps selectedDateString}}</div>
<ul class="list-unstyled list-rooms">
	{{#each item in rooms}} 
		<li>
			{{room-item room=item selectedDay=selectedDay}}
		</li>
		{{else}}
			<p>{{t 'filter.noResultForActiveFilter'}}</p>
	{{/each}}
</ul>


